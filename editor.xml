<application>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern useCustomColors="true" pattern="\bimportant\b.*">
      <option name="FOREGROUND" value="46bd2a" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <pattern icon="question" useCustomColors="true" pattern="\bnote\b.*">
      <option name="FOREGROUND" value="4198ff" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern icon="important" useCustomColors="true" pattern="\bcritical\b.*">
      <option name="FOREGROUND" value="f5f2ff" />
      <option name="BACKGROUND" value="ee840c" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern icon="important" useCustomColors="true" pattern="\bbug\b.*">
      <option name="FOREGROUND" value="f0f0f3" />
      <option name="BACKGROUND" value="e32225" />
      <option name="FONT_TYPE" value="1" />
    </pattern>
  </component>
</application>